    <script type="text/javascript">
        $(document).ready(function(){
            $('#search').click(function(){
                let market_type = $('#market_type').val();
                let floor = $('#floor').val();
                let stall_name = $('#stall_name').val();
                let customer_name = $('#customer_name').val();
                console.log("market_type", market_type);
                console.log("floor", floor);
                console.log("stall_name", stall_name);
                console.log('customer_name', customer_name);
                window.location.href = `/query/stall?market_type=${market_type}&floor=${floor}&stall_name=${stall_name}&customer_name=${customer_name}`;
            });

            $('#importBtn').click(function(){
                const file = document.getElementById('file1');
                console.log("file", file);
                if(file.files[0] == "" || file.files[0] == undefined){
                    alert("请先选择文件");
                }else{   
                    const formData = new FormData();
                    formData.append("excel", file.files[0]);
                    console.log("import", file);
                    console.log("import2", file.files[0]);
                    $.ajax({
                        url: "/stall/excel/import",
                        type: "post",
                        data: formData,
                        processData: false,
                        contentType: false,
                        success: function(result){
                            if(result.info){
                                setTimeout(function(){
                                    alert("导入成功");
                                    window.location.href= "/stall";
                                }, 3000);
                            }
                        },
                        error: function(err){
                            console.log("err", err);
                        }
                    });
                }
            });

            $('#exportBtn').click(function(){
                const start = $('#start').val();
                const end = $('#end').val();
                if(start=="" || end == ""){
                    alert("导出的范围请填完整");
                }else{
                    console.log("start", start);
                    console.log("end", end);
                    window.location.href = "/stall/excel/export/"+start+"/"+end;
                }
            });

            $('#updateSubmit').click(function(){
                const id = $('#update_customer_id').val();
                const customer_type = $('#update_customer_type').val();
                const market_type=$('#update_market_type').val();
                const floor = $('#update_floor').val();
                const stall_name = $('#update_stall_name').val();
                const customer_name = $('#customer_name').val();
                let phone="";
                const phone2 = $('#update_phone2').val();
                const phone3 = $("#update_phone3").val();
                if(phone3 != "" || phone3 != undefined ){
                    phone = phone2+","+phone3;
                }
                const identity_card = $('#update_identity2').val();
                let remark= $('#update_remark').val();
                if(remark.indexOf(".">0)){
                    remark = remark.split("·");
                    remark = remark[0]+""
                }             
                console.log("remark", remark);
                $.ajax({
                    url: "/query/stall/"+id,
                    type: "put",
                    data: {
                        "customer_type": customer_type,
                        "market_type": market_type,
                        "floor": floor,
                        "stall_name":stall_name,
                        "customer_name": customer_name,
                        "phone": phone,
                        "identity_card": identity_card,
                        "remark": remark
                    },
                    success: function(result){
                        if(result.info){
                            alert("修改成功");
                            window.location.href= "/stall"
                        }
                    },
                    error: function(err){
                        alert("修改失败");
                        console.log(err);
                    }
                });
            })

            $('#addSubmit').click(function(){
                const customer_type = $("#add_customer_type").val();
                const market_type = $("#add_market_type").val();
                const floor = $("#add_floor").val();
                const stall_name = $("#add_stall_name").val();
                const customer_name = $("#add_customer_name").val();
                const phone2 = $("#add_phone2").val();
                const phone3 = $("#add_phone3").val();
                const phone = phone2+","+phone3;
                const identity_card = $("#add_identity").val();
                const remark = $("#add_remark").val();
                $.post("/stall/add", {
                    "customer_type":customer_type,
                    "market_type" : market_type,
                    "floor": floor,
                    "stall_name": stall_name,
                    "customer_name": customer_name,
                    "phone": phone,
                    "identity_card": identity_card,
                    "remark": remark
                },function(result){
                    if(result.info){
                        alert("添加成功");
                        window.location.href="/stall"
                    }else{
                        alert("该记录已经存在了");
                    }
                })
            });
            
            $('#more').click(function(){
                let pageCount = "<%= pageCount%>";
                console.log("pageCount", pageCount);
                $('#more').css("display", "none");
                for(let i=20; i<pageCount; i++){
                    $('#page li').eq(i).after("<li><a href='/stall/"+i+"'>"+i+"</a></li>")
                }
            });

        });
        function lookMsg(id){
            $.get("/query/stall/"+id, function(result){
                if(result.info){
                    const stall_detail = result.stall_detail;
                    console.log("stall_detail", stall_detail);
                    $("#customer_id2").val(stall_detail.id);
                    $("#customer_type2").val(stall_detail.customer_type);
